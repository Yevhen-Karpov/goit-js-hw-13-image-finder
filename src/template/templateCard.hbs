{{#each this}}
<li>
     <div class="photo-card">
    <img src="{{webformatURL}}" alt="" class="img"  data-src = '{{largeImageURL}}'/>

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i><br>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i><br>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i><br>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i><br>
            {{downloads}}
        </p>
    </div>
</div>
</li>
{{/each}}